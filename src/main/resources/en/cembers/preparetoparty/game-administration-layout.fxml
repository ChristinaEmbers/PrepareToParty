<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox prefHeight="558.0" prefWidth="802.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="en.cembers.preparetoparty.gui.GameController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <children>
        <HBox prefHeight="100.0" prefWidth="200.0">
            <children>
                <HBox prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                    <children>
                        <Label fx:id="lblChooseCategory" prefHeight="18.0" prefWidth="180.0" text="Choose Food"
                               HBox.hgrow="ALWAYS">
                            <HBox.margin>
                                <Insets/>
                            </HBox.margin>
                            <font>
                                <Font size="14.0"/>
                            </font>
                            <padding>
                                <Insets left="10.0"/>
                            </padding>
                        </Label>
                    </children>
                </HBox>
                <HBox prefHeight="18.0" prefWidth="227.0" HBox.hgrow="ALWAYS">
                    <children>
                        <Label alignment="CENTER_RIGHT" text="Time">
                            <HBox.margin>
                                <Insets right="5.0"/>
                            </HBox.margin>
                        </Label>
                        <Label fx:id="lblTime" text="100">
                            <HBox.margin>
                                <Insets right="5.0"/>
                            </HBox.margin>
                        </Label>
                        <Label text="Energy">
                            <HBox.margin>
                                <Insets right="5.0"/>
                            </HBox.margin>
                        </Label>
                        <Label fx:id="lblEnergy" text="100">
                            <HBox.margin>
                                <Insets right="5.0"/>
                            </HBox.margin>
                        </Label>
                        <Label text="Money">
                            <HBox.margin>
                                <Insets right="5.0"/>
                            </HBox.margin>
                        </Label>
                        <Label fx:id="lblMoney" alignment="CENTER_RIGHT" text="100">
                            <HBox.margin>
                                <Insets right="5.0"/>
                            </HBox.margin>
                        </Label>
                    </children>
                </HBox>
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="331.0" prefWidth="762.0">
            <children>
                <VBox prefHeight="248.0" prefWidth="550.0" HBox.hgrow="ALWAYS">
                    <children>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="275.0">
                            <children>
                                <HBox alignment="CENTER_LEFT" prefHeight="76.0" prefWidth="180.0" HBox.hgrow="ALWAYS">
                                    <children>
                                        <Button fx:id="btnAction1" alignment="TOP_LEFT" mnemonicParsing="false"
                                                onAction="#executeAction1" text="Button">
                                            <HBox.margin>
                                                <Insets left="20.0"/>
                                            </HBox.margin>
                                        </Button>
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                                    <children>
                                        <Label fx:id="lblAction1" text="Label"/>
                                    </children>
                                </HBox>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="275.0">
                            <children>
                                <HBox alignment="CENTER_LEFT" prefHeight="76.0" prefWidth="180.0" HBox.hgrow="ALWAYS">
                                    <children>
                                        <Button fx:id="btnAction2" alignment="TOP_LEFT" mnemonicParsing="false"
                                                onAction="#executeAction2" text="Button">
                                            <HBox.margin>
                                                <Insets left="20.0"/>
                                            </HBox.margin>
                                        </Button>
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                                    <children>
                                        <Label fx:id="lblAction2" text="Label"/>
                                    </children>
                                </HBox>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" layoutX="10.0" layoutY="110.0" prefHeight="100.0" prefWidth="275.0">
                            <children>
                                <HBox alignment="CENTER_LEFT" prefHeight="76.0" prefWidth="180.0" HBox.hgrow="ALWAYS">
                                    <children>
                                        <Button fx:id="btnAction3" alignment="TOP_LEFT" mnemonicParsing="false"
                                                onAction="#executeAction3" text="Button">
                                            <HBox.margin>
                                                <Insets left="20.0"/>
                                            </HBox.margin>
                                        </Button>
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                                    <children>
                                        <Label fx:id="lblAction3" text="Label"/>
                                    </children>
                                </HBox>
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </children>
        </HBox>
        <HBox prefHeight="333.0" prefWidth="762.0">
            <children>
                <TextArea fx:id="logbook" prefHeight="251.0" prefWidth="763.0"
                          text="Time to prepare the most epic birthday party ever! Try to find the best combination!&#10;"/>
            </children>
        </HBox>
    </children>
</VBox>
